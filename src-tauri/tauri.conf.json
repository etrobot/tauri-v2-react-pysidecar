{
  "$schema": "gen/schemas/desktop-schema.json",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": ["bin/main"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "shortDescription": "",
    "longDescription": "",
    "resources": [],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "entitlements": "entitlements.plist",
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": "-"
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "tauri-python-sidecar",
  "mainBinaryName": "tauri-python-sidecar",
  "identifier": "com.autobiu.ashare",
  "plugins": {
    "http": {
      "all": true,
      "scope": [
        "http://localhost:61125/*",
        "http://127.0.0.1:61125/*"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 950,
        "resizable": true,
        "title": "tauri-python-sidecar",
        "width": 1200,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
